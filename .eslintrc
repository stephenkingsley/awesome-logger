{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
  },
  "rules": {
    "arrow-body-style": 0,
    "no-console": 0,
    "strict": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "object-shorthand": 0
  }
}
